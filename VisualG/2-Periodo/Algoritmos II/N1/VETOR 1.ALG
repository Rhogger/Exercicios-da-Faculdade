algoritmo "Vetor 1"

var
   vetorA: vetor[0..8] de inteiro
   contador: inteiro

inicio
   para contador de 0 ate 8 faca
      escreva("Insira um valor à ser armazenado: ")
      leia(vetorA[contador])
      escreval(" ")
   fimpara
   para contador de 8 ate 0 passo -1 faca
      escreval(vetorA[contador])
   fimpara

fimalgoritmo